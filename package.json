{
  "name": "portfolio-api",
  "version": "1.0.0",
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun run --watch src/app.ts",
    "db:gen": "bunx drizzle-kit generate",
    "db:migrate": "bunx drizzle-kit migrate",
    "db:reset": "bun src/script/reset-db.ts",
    "lint": "bunx eslint .",
    "uml:gen": "bun run src/script/generator/generate-uml.ts",
    "uml:gen:category": "bun run src/script/generator/generate-uml.ts",
    "uml:gen:file": "bun run src/script/generator/generate-uml.ts",
    "docker:up": "docker compose --env-file .env -f infrastructure/docker/docker-compose.yml up -d",
    "docker:down": "docker compose --env-file .env -f infrastructure/docker/docker-compose.yml down",
    "docker:logs": "docker compose --env-file .env -f infrastructure/docker/docker-compose.yml logs -f"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.886.0",
    "@axiomhq/js": "^1.3.1",
    "@bogeychan/elysia-logger": "^0.1.10",
    "@elysiajs/cookie": "^0.8.0",
    "@elysiajs/cors": "^1.3.3",
    "@elysiajs/swagger": "^1.3.1",
    "@neondatabase/serverless": "^1.0.1",
    "@sentry/bun": "^10.3.0",
    "@types/jsonwebtoken": "^9.0.10",
    "arctic": "^3.7.0",
    "chalk": "^5.5.0",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "elysia": "latest",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "logixlysia": "^5.1.0",
    "nodemailer": "^7.0.6",
    "pg": "^8.16.3",
    "pino": "^9.8.0",
    "pino-pretty": "^13.1.1",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0",
    "typeorm": "^0.3.25",
    "ua-parser-js": "^2.0.4",
    "winston": "^3.17.0",
    "winston-transport": "^4.9.0",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/js-yaml": "^4.0.9",
    "@types/nodemailer": "^7.0.1",
    "bun-types": "latest",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.33.0",
    "globals": "^16.3.0",
    "node-plantuml": "^0.9.0",
    "prettier": "^3.6.2",
    "semver": "^7.7.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  },
  "module": "src/index.js"
}
